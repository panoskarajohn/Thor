{
  "app": {
    "name": "Game.Api",
    "service": "game-api",
    "version": "1.0.0"
  },
  "logger": {
    "applicationName": "event-service",
    "excludePaths": [ "/ping", "/metrics" , "/healthcheck" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "hour"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "redis": {
    "ConnectionString": "localhost:6379",
    "LockExpirationTime": "00:00:30"
  },
  "AllowedHosts": "*"
}
